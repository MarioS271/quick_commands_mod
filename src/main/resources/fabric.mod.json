{
	"schemaVersion": 1,
	"id": "quick_commands",
	"version": "${version}",
	"name": "QuickCommands",
	"description": "A minecraft mod with GUIs for easy access to useful commands.",
	"authors": [
		"MarioS271"
	],
	"contact": {
		"homepage": "https://modrinth.com/mod/quickcommands",
		"sources": "https://github.com/MarioS271/quick_commands_mod",
		"issues": "https://github.com/MarioS271/quick_commands_mod/issues"
	},
	"license": "CC0-1.0",
	"icon": "assets/quick_commands/icon.png",
	"environment": "client",
	"accessWidener" : "quick_commands.accesswidener",
	"entrypoints": {
		"main": [
			"net.marios271.quick_commands.QuickCommands"
		],
		"client": [
			"net.marios271.quick_commands.QuickCommandsClient"
		]
	},
	"depends": {
		"fabricloader": ">=0.14.0",
		"minecraft": "~1.21",
		"java": ">=21",
		"fabric": "*",
